query = """
SELECT DISTINCT
    s.state_name
FROM AQI_Record a
JOIN State s
  ON a.state_code = s.state_code
WHERE a.aqi > (
    SELECT AVG(aqi)
    FROM AQI_Record
    WHERE category = 'Good'
);
"""

cur.execute(query)
results = cur.fetchall()
results



